{
  "name": "ecommerce-automation",
  "version": "1.0.0",
  "description": "E-commerce and Automation Platform with AI-Powered Features",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "install:client": "npm install",
    "install:api": "cd api && npm install",
    "install:all": "npm run install:client && npm run install:api",
    "build": "vite build",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "cd api && npm run dev",
    "dev:client": "vite --port 3000",
    "start:api": "cd api && npm start",
    "seed:api": "cd api && npm run seed",
    "start": "cd api && npm start",
    "preview": "vite preview",
    "postinstall": "npm run install:all && npm run build"
  },
